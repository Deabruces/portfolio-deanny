{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"],
      "@icons/*": ["assets/icons/*"],
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@styles/*": ["assets/styles/*"],
      "@images/*": ["assets/images/*"],
      "@scripts/*": ["scripts/*"]
    }
  }
}
